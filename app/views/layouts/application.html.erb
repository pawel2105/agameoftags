<!DOCTYPE html>
<html>
  <head>
    <title>A Game of Tags</title>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Raleway', media: "all" %>
    <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Lato:300', media: "all" %>
    <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Open+Sans:300,400,700', media: "all" %>
    <%= csrf_meta_tags %>

    <%= render partial: 'shared/meta' %>
    <% if Rails.env.production? %>
      <%= render partial: 'shared/ga' %>
    <% end %>
  </head>

  <body>
    <div class="body-table-container">
      <%= render partial: 'shared/nav' %>
      <%= yield %>
      <div style="display: table-row;height: 1px;"></div>
      <%= render partial: 'shared/footer' %>
    </div>
  </body>
</html>
